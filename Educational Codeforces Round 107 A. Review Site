#include<bits/stdc++.h>
using namespace std;
 
int main(){
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
		int n, tmp;
		cin>>n;
		int up=0, down=0;
		for(int i=0;i<n;i++){
			cin>>tmp;
			if(tmp==1 || tmp==3)
			   up++;
			else
			   down++;
		}
		cout<<up<<endl;
	}
	return 0;
}
