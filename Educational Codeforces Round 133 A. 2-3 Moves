#include<bits/stdc++.h>
using namespace std;
 
int main(){
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
		int n, ans;
		cin>>n;
		if(n==1){
			cout<<2<<endl;
			continue;
		}
		if(n%3==0){
			cout<<n/3<<endl;
		}
		else{
			cout<<n/3+1<<endl;
		}
	}
	return 0;
}
